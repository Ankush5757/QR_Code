{
  "builds": [
    {
      "src": "PROJECT/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10.11",
        "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --no-input"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "PROJECT/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_DEBUG": "False",
    "DJANGO_ALLOWED_HOSTS": "App.vercel.app",
    "PYTHON_VERSION": "3.10.11"
  }
}